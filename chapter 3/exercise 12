import turtle

wn = turtle.Screen()
wn.title("A face of a clock")
wn.bgcolor("lightgreen")

tunkuff = turtle.Turtle()
tunkuff.pensize(3)
tunkuff.speed(0)
tunkuff.shape("turtle")
tunkuff.color("blue")

for i in range(12):
    tunkuff.penup()
    tunkuff.forward(80)
    tunkuff.pendown()
    tunkuff.forward(10)
    tunkuff.penup()
    tunkuff.forward(30)
    tunkuff.stamp()
    tunkuff.backward(120)
    tunkuff.right(30)

tunkuff.stamp()

wn.mainloop()
